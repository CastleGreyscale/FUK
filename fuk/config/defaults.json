  "postprocess": {
    "upscale": {
      "model": "real_esrgan_x4",
      "scale": 4
    },
    "interpolate": {
      "model": "rife",
      "multiplier": 2
    }
  },

  "export": {
    "output_format": "png",
    "create_exr": true,
    "exr_linear": true,
    "include_metadata": true
