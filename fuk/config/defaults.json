{
  "_comment": "FUK User Defaults - Per-tab generation settings and project preferences",

  "aspect_ratios": {
    "_comment": "Aspect ratio presets - label maps to ratio for height calculation",
    "Square": 1.0,
    "Fullscreen": 1.33,
    "Academy": 1.85,
    "Widescreen": 1.78,
    "Anamorphic": 2.39,
    "Panavision": 2.75
  },

  "image": {
    "positive_prompt": "",
    "negative_prompt": "blurry, low quality, distorted, deformed, ugly, bad anatomy, worst quality, low resolution, jpeg artifacts, watermark, signature, username, text",
    "width": 1344,
    "aspect_ratio": "Widescreen",
    "infer_steps": 20,
    "guidance_scale": 2.1,
    "flow_shift": 2.1,
    "blocks_to_swap": 0,
    "lora_multiplier": 1.0,
    "seed": null
  },

  "video": {
    "positive_prompt": "",
    "negative_prompt": "blurry, low quality, distorted, deformed, ugly, bad anatomy, worst quality, low resolution, jpeg artifacts, watermark, signature, username, text",
    "size": [832, 480],
    "length": 81,
    "infer_steps": 20,
    "guidance_scale": 5.0,
    "flow_shift": 5.0,
    "blocks_to_swap": 0,
    "lora_multiplier": 1.0,
    "seed": null
  },

  "preprocess": {
    "depth": {
      "model": "da3_mono_large",
      "invert": false,
      "normalize": true,
      "colormap": "inferno"
    },
    "normals": {
      "method": "depth_gradient",
      "depth_model": "da3_mono_large",
      "blur_radius": 3,
      "normalize": true
    },
    "canny": {
      "low_threshold": 100,
      "high_threshold": 200
    },
    "openpose": {
      "include_hands": true,
      "include_face": true
    },
    "crypto": {
      "model": "sam2",
      "mode": "auto",
      "points_per_side": 32,
      "pred_iou_thresh": 0.86,
      "stability_score_thresh": 0.92
    }
  },

  "postprocess": {
    "upscale": {
      "model": "real_esrgan_x4",
      "scale": 4
    },
    "interpolate": {
      "model": "rife",
      "multiplier": 2
    }
  },

  "export": {
    "output_format": "png",
    "create_exr": true,
    "exr_linear": true,
    "include_metadata": true
  },

  "project": {
    "shot_label": "shot",
    "versioning": "DATE",
    "autosave_interval": 30,
    "cache_cleanup_days": 7
  }
}